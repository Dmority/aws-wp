terraform {
  required_providers {
    aws = {
      source  = "hashicorp/aws"
      version = "~> 3.0"
    }
  }
}
provider "aws" {
  region = "ap-northeast-1"
  default_tags {
    tags = {
      CostCenter = "poc"
    }
  }
  backend "s3" {
      bucket = ${BACKEND_BUCKET}
      key    = "terraform.state"
  }
}
